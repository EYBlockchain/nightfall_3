from "hashes/sha256/1536bit.code" import main as sha256of1536

def main(field[1536] a) -> (field[256]):
    field[256] output = sha256of1536(\
        a[0..256],\
        a[256..512],\
        a[512..768],\
        a[768..1024],\
        a[1024..1280],\
        a[1280..1536]\
    )
    return output
