{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
      "includePattern": ".+\\.mjs(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
          "destination": "doc/lib"
    }
}
