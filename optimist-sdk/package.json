{
  "name": "proposer",
  "version": "1.0.0",
  "description": "'makes a container with proposer functionality'",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --trace-warnings src/app.mjs",
    "build:esm": "tsc --module es6 -outDir dist/esm",
    "build:cjs": "tsc --module CommonJS -outDir dist/cjs",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "dev": "nodemon -e mjs,yaml,json src/app.mjs",
    "test": "NODE_ENV=development jest"
  },
  "author": "westlad",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/config": "^3.3.0",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "babel-jest": "^29.1.2",
    "body-parser": "^1.19.2",
    "cls-hooked": "^4.2.2",
    "common-files": "file:../common-files",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-fileupload": "^1.4.0",
    "nodemon": "^2.0.20",
    "pino": "^8.3.1",
    "pino-pretty": "^8.1.0",
    "queue": "^6.0.2",
    "reconnecting-websocket": "^4.4.0",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "uuid": "^8.3.2",
    "web3": "^1.7.5",
    "ws": "^8.2.3",
    "yaml": "^2.1.1"
  },
  "overrides": {
    "swarm-js": {
      "got": "11.8.5"
    }
  },
  "devDependencies": {
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
