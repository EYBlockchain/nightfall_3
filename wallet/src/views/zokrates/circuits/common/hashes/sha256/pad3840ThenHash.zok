from "./hash4096" import main as sha256

// Add padding to a 2560-bit message, then hash it.
def main(u32[120] a)->(u32[8]):

	u32[8] padding = [ \
        0x80000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
				0x00000F00
    ]

	return sha256([...a, ...padding])
