from "./hash1024" import main as sha256

// Add padding to a 896-bit message, then hash it.
def main(u32[24] a)->(u32[8]):

	u32[8] padding = [\
		0x80000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000300
    ]

	return sha256([...a, ...padding])
