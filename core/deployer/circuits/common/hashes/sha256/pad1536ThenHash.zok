from "./hash2048" import main as sha256

// Add padding to a 640-bit message, then hash it.
def main(u32[48] a)->(u32[8]):

	u32[16] padding = [ \
        0x80000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
	    0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
		0x00000000,
        0x00000000,
        0x00000600
    ]

	return sha256([...a, ...padding])
