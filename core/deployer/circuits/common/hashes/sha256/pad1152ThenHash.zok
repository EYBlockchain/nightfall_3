from "./hash1536" import main as sha256

// Add padding to a 1152-bit message, then hash it.
def main(u32[36] a)->(u32[8]):

	u32[12] padding = [ \
        0x80000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
		0x00000000,
		0x00000000,
		0x00000000,
		0x00000000,
        0x00000480
    ]

	return sha256([...a, ...padding])
