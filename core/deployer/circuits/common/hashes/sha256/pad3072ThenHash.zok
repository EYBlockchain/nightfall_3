from "./hash3584" import main as sha256

// Add padding to a 2560-bit message, then hash it.
def main(u32[96] a)->(u32[8]):

	u32[16] padding = [ \
        0x80000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
	      0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
        0x00000000,
		    0x00000000,
        0x00000000,
		    0x00000C00
    ]

	return sha256([...a, ...padding])
