from "./hash1536" import main as sha256

// Add padding to a 1280-bit message, then hash it.
def main(u32[40] a)->(u32[8]):

	u32[8] padding = [ \
        0x80000000,
		0x00000000,
		0x00000000,
		0x00000000,
		0x00000000,
		0x00000000,
		0x00000000,
		0x00000500
    ]

	return sha256([...a, ...padding])
