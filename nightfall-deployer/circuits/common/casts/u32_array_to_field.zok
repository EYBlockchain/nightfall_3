from "EMBED" import u32_to_bits;

def main<N>(u32[N] i) -> field {
    field mut res = 0;
    for u32 k in 0..N {
        for u32 j in 0..32 {
            bool[32] bits = u32_to_bits(i[k]);
            u32 exponent = (N - k - 1) * 32 + (32 - j - 1);
            res = res + if bits[j] { 2 ** exponent } else { 0 };
        }
    }
    return res;
}
