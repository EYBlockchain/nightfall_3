{
  "name": "nightfall_3",
  "version": "0.0.1",
  "description": "Optimistic Nightfall",
  "main": "./nightfall-client/src/index.mjs",
  "scripts": {
    "test": "LOG_LEVEL=error mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/e2e/protocol/*.test.mjs test/e2e/tokens/*.test.mjs",
    "neg-test": "mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/neg-http.mjs",
    "neg-test-ropsten": "mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/neg-http.mjs",
    "test-e2e-protocol": "LOG_LEVEL=error mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/e2e/protocol/*.test.mjs ",
    "test-gas": "LOG_LEVEL=error mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/e2e/gas.test.mjs ",
    "test-e2e-tokens": "LOG_LEVEL=error mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/e2e/tokens/*.test.mjs ",
    "test-erc20-tokens": "LOG_LEVEL=error mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/e2e/tokens/erc20.test.mjs ",
    "test-erc20-cli": "LOG_LEVEL=debug mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/client/erc20.test.mjs ",
    "lint": "eslint . --ext js,mjs,jsx,ts,tsx && find-unused-exports",
    "prepare": "husky install",
    "doc:build:sdk": "jsdoc -c jsdoc.json cli/lib/nf3.mjs",
    "build-adversary": "node test/adversary/transpile-adversary.mjs",
    "adversary-test-all": "for CHALLENGE_TYPE in IncorrectTreeRoot IncorrectLeafCount DuplicateTransaction DuplicateNullifier HistoricRootError IncorrectProof; do CHALLENGE_TYPE=${CHALLENGE_TYPE} mocha --timeout 0 --bail --exit test/adversary.test.mjs; sleep 5; done",
    "secrets-encryption-test": "mocha --timeout 0 --bail --exit test/encryption.test.mjs",
    "adversary-test": "ETH_NETWORK=development CHALLENGE_TYPE=${CHALLENGE_TYPE} mocha --require test/utils/deployMocks.js --timeout 0 --bail --exit test/adversary.test.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EYBlockchain/optimistic-nightfall.git"
  },
  "author": "duncan.westland@uk.ey.com",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/EYBlockchain/optimistic-nightfall/issues"
  },
  "homepage": "https://github.com/EYBlockchain/optimistic-nightfall#readme",
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@openzeppelin/contracts": "^4.3.2",
    "async-mutex": "^0.3.2",
    "axios": "^0.21.4",
    "babel-eslint": "^10.1.0",
    "bip39": "^3.0.4",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-things": "^0.2.0",
    "common-files": "file:./common-files",
    "config": "^3.3.1",
    "eslint": "^7.25.0",
    "eslint-config-codfish": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "fast-check": "^2.18.0",
    "find-unused-exports": "^2.0.0",
    "general-number": "^1.0.1",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.10",
    "mocha": "^10.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "queue": "^6.0.2",
    "reconnecting-websocket": "^4.4.0",
    "web3": "^1.4.0",
    "ws": "^7.5.0"
  },
  "dependencies": {
    "async-mutex": "^0.3.2",
    "docker-compose": "^0.23.13",
    "general-number": "^1.0.1",
    "typescript": "^4.6.2"
  },
  "overrides": {
    "got": "11.8.5"
  }
}
