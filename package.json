{
  "name": "nightfall-client",
  "version": "1.3.0",
  "description": "A client for interacting with Nightfall",
  "main": "index.mjs",
  "scripts": {
    "test": "npx mocha --timeout 0",
    "start": "node src/index.mjs",
    "dev": "nodemon src/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EYBlockchain/nightfall-client.git"
  },
  "keywords": [
    "Nightfall",
    "zkp"
  ],
  "author": "westlad",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/EYBlockchain/nightfall-client/issues"
  },
  "homepage": "https://github.com/EYBlockchain/nightfall-client#readme",
  "eslintConfig": {
    "extends": [
      "./node_modules/cod-scripts/eslint.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cod-scripts pre-commit",
      "commit-msg": "cod-scripts commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@semantic-release/git": "^9.0.0",
    "amqplib": "^0.6.0",
    "axios": "^0.20.0",
    "body-parser": "1.19.0",
    "config": "^3.3.1",
    "cors": "2.8.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.4",
    "general-number": "^1.0.1",
    "hex-to-binary": "^1.0.1",
    "keccak": "^3.0.1",
    "mongodb": "^3.6.2",
    "tar": "^6.0.2",
    "web3": "^1.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "mocha": "^8.1.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cod-scripts": "^3.2.0",
    "eslint": "^7.9.0",
    "markdownlint-cli": "^0.15.0",
    "nodemon": "^1.18.9",
    "supertest": "^4.0.2"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "output/",
      "circuits/",
      "*.json"
    ]
  }
}
